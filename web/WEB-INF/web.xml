<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>view.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>homeDoctor</servlet-name>
        <servlet-class>view.HomeDoctor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homeDoctor</servlet-name>
        <url-pattern>/homeDoctor</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>profile</servlet-name>
        <servlet-class>view.Profile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>homePatient</servlet-name>
        <servlet-class>view.HomePatient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homePatient</servlet-name>
        <url-pattern>/homePatient</url-pattern>
    </servlet-mapping>
        <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>view.SignUp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
